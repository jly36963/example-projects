{
  "name": "ts-nextjs",
  "version": "1.0.0",
  "description": "Typescript React Express Nextjs Material-UI",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "tsc": "tsc",
    "ts-dev": "nodemon --exec 'ts-node' server.ts",
    "ts-dev2": "nodemon --exec 'tsc && ts-node' server.ts",
    "custom-dev": "nodemon server.ts",
    "build": "tsc && next build",
    "start": "tsc && NODE_ENV=production node server.js"
  },
  "author": "Landon Yarrington",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "next": "^9.5.3",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@zeit/next-typescript": "^1.1.1",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  }
}
